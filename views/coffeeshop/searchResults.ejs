<h1>Coffeeshops:</h1>

<div class="results">
    <%  coffeeshops.forEach(coffeeshop => { %>
        <div class="gridDiv">
            
        <a href="/coffeeshops/<%= coffeeshop.id %>?yelpId=<%= coffeeshop.yelpId %>">
            <img src="<%= coffeeshop.image_url %>" class="coffeeshopImg"/>
            <h3><%= coffeeshop.name %></h3>
        </a>
        <h5 class="nameOfCoff"><%= coffeeshop.location.display_address %></h5>
        
        <form action="/coffeeshops" method="POST">
            <input type="hidden" name="name" value="<%= coffeeshop.name %>">
            <input type="hidden" name="yelpId" value="<%= coffeeshop.id %> ">
            <input type="hidden" name="img" value="<%= coffeeshop.image_url %> ">
            <input type="submit" value="❤️ to favorites" class="favoriteButt">
        </form>
    </div>
    <% }) %> 
</div>